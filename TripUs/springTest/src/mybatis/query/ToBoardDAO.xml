<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		 	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tu.toboard.dao.ToBoardMapper">
	<resultMap type="com.tu.toboard.vo.ToBoardVO" id="ToBoardVO">
		<result property="viewname" column="VIEWNAME"/>
		<result property="viewno" column="VIEWNO"/>
		<result property="contentid" column="CONTENTID"/>
	</resultMap>
	 
	
	
	<select id="selectContentID" parameterType="com.tu.toboard.vo.ToBoardVO" resultMap="ToBoardVO">
		/* Mapper - selectContentID */
			SELECT CONTENTID
			FROM TOURLIST
			WHERE VIEWNO=#{viewno} 
	</select>
	<select id="toBoardList" parameterType="com.tu.toboard.vo.ToBoardVO" resultMap="ToBoardVO">
		/* Mapper - coBoardList */
			SELECT CONTENTID,VIEWNAME,VIEWNO
			FROM TOURLIST
			
	</select>
	
</mapper>